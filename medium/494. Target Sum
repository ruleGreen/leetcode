method one: Time Limit Exceed

class Solution:
    def findTargetSumWays(self, nums: List[int], S: int) -> int:
        self.S = S
        self.count = 0 
        
        # recursion
        self.getSumWays(nums, 0, '')
        
        return self.count
    
    
    def getSumWays(self, nums, s, symbols):
        if s == self.S and len(nums) == 0:
            self.count += 1
            return
        elif len(nums) == 0:
            return 
        
        self.getSumWays(nums[1:], s + nums[0], '+')
        self.getSumWays(nums[1:], s - nums[0], '-')
        
