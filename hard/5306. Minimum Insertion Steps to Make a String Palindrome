class Solution {
public:
    int minInsertions(string s) {
        int n = s.size();
        /*
        vector<int> v(255);
        for (int i = 0; i < n; ++ i)
            v[s[i]].push_back(i);
            */
        const int INF = 1000000000;
        vector<vector<int>> f(n, vector<int>(n));
        for (int i = n-1; i >= 0; -- i)
        {
            f[i][i] = 0;
            for (int j = i+1; j < n; ++ j)
            {
                if (s[i] == s[j])
                    f[i][j] = f[i+1][j-1];
                else
                {
                    f[i][j] = min(f[i+1][j], f[i][j-1])+1;
                }
            }
        }
        return f[0][n-1];
    }
};
