1.

class Solution:
    def missingNumber(self, nums: List[int]) -> int:
        if len(nums) <= 0:
            return 0
        if len(nums) == max(nums) + 1:
            return max(nums) + 1
        flag = [0 for i in range(max(nums)+1)]
        for i in range(len(nums)):
            flag[nums[i]] = 1
        for i in range(len(flag)):
            if flag[i] == 0:
                return i
        
