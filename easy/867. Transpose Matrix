class Solution:
    def transpose(self, A: List[List[int]]) -> List[List[int]]:
        transpose = [ [0 for j in range(len(A))] for i in range(len(A[0]))]
        for i in range(len(A)):
            for j in range(len(A[0])):
                transpose[j][i] = A[i][j]
        return transpose
        
