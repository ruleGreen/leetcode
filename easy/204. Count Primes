class Solution:
    def countPrimes(self, n: int) -> int:
        res = []
        for i in range(2,n):
            if self.isPrimes(i):
                res.append(i)
        return len(res)
    
    def isPrimes(self, n):
        for i in range(2,n):
            if n%i == 0:
                return False
        return True
