"""
# Definition for a Node.
class Node:
    def __init__(self, val, children):
        self.val = val
        self.children = children
"""
class Solution:
    def levelOrder(self, root: 'Node') -> List[List[int]]:
        self.res = []
        if not root:
            return self.res
        queue,k = [root],0
        while len(queue) > 0:
            queue_new = []
            # print("1", self.res, k)
            if len(self.res) < k + 1:
                self.res.append([])
            # print("2", self.res, k)
            for i in range(0, len(queue)):
                self.res[k].append(queue[i].val)
                queue_new.extend(queue[i].children)
            queue = queue_new
            k += 1
        return self.res
            
        
        
